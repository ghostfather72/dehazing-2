I = imread('Data\seed.png');
[h,w,s] = size(I);

if(s == 3)
    I = rgb2hsv(im2double(I));
    I = I(:,:,3);
end

rata = mean(mean(I));
stddev = std(std(I));
tau = 3;
a = rata + 2 * stddev;
b = rata - 2 * stddev;
D = a - b;

islow = 0;
if(D <=  1 / tau) 
    islow = 1;
end

y = 0;

if(islow)
    y = -log2(stddev);
    
else
end

